generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      =  env("DATABASE_URL")
 
}

model Cliente {
  id        String   @id @default(cuid())
  nome      String
  telefone  String
  cpf       String @unique @db.VarChar(11)
  createdAt DateTime @default(now())
}